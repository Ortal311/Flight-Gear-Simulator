<AnchorPane prefHeight="105.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view_PlayerButtons.PlayerButtonsController">
   <children>
      <Button layoutX="110.0" layoutY="5.0" mnemonicParsing="false" onAction="#onOpenXML" prefHeight="30.0" prefWidth="90.0" text="Choose XML" />
      <Button layoutX="10.0" layoutY="5.0" mnemonicParsing="false" onAction="#onOpen" prefHeight="30.0" prefWidth="90.0" text="Choose CSV" />
      <Button layoutX="10.0" layoutY="45.0" mnemonicParsing="false" onAction="#onPlay" prefHeight="30.0" prefWidth="50.0" text="Play" />
