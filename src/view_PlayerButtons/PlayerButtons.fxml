      <Text layoutX="565.0" layoutY="64.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Time :" wrappingWidth="50.0">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <TextField fx:id="timeFlight" layoutX="625.0" layoutY="45.0" prefHeight="29.0" prefWidth="76.0" promptText="00:00:00">
